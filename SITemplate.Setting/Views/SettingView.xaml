<UserControl x:Class="SITemplate.Setting.Views.SettingView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:cvsWindow="clr-namespace:CvsService.UI.Windows.UI.Units;assembly=CvsService.UI.Windows"
             xmlns:local="clr-namespace:SITemplate.Setting.Views"
             mc:Ignorable="d" 
             d:DesignHeight="890" d:DesignWidth="1900">
    <Grid>
        <cvsWindow:Depth1Panel FontSize="20">
            <StackPanel>
                <Label Content="Setting View"/>
                <StackPanel Orientation="Horizontal">
                    <Label Content="Theme : "/>
                    <!--<ComboBox Width="350" FontSize="15"
                              ItemsSource="{Binding LanguageStrings, Source={StaticResource LanguageValueConverter}}"
                              SelectedIndex="{Binding LanguageSet.Language, Converter={StaticResource LanguageValueConverter}}"/>-->
                </StackPanel>
            </StackPanel>
        </cvsWindow:Depth1Panel>
    </Grid>
</UserControl>
